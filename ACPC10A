#include<stdio.h>
int main()
{ int a1,a2,a3;
scanf("%d",&a1);
scanf("%d",&a2);
scanf("%d",&a3);
while(1)
{
if(a1==0&&a2==0&&a3==0)
{ break;
}
else if((2*a2)==(a3+a1))
{ printf("AP  %d\n",(a3+(a2-a1)));
}
else
{ printf("GP  %d\n",(a3*(a2/a1)));
}
scanf("%d",&a1);
scanf("%d",&a2);
scanf("%d",&a3);
} return 0;
}
